<script>
  function openImage() {
    window.location.href = 'https://scontent.fbnu2-1.fna.fbcdn.net/v/t1.6435-9/90245277_1927290557422634_7149493876221280256_n.jpg?_nc_cat=108&ccb=1-7&_nc_sid=5f2048&_nc_eui2=AeEDaY9CraPJdbYxIIsz-uFhu3DksGCR6Gm7cOSwYJHoaaOQTqqyFFmCO9GaormfMLfthIEL2ltAE2OQ_AJ96LOJ&_nc_ohc=xh4xkYTYiA8Ab5-ir9R&_nc_ht=scontent.fbnu2-1.fna&oh=00_AfDyXPEYIul71dPZABUNVihGNSD5OhwZdP_9IIQ1Q8ylAw&oe=663E8161';
    redirectToYouTube();
  }

  function redirectToYouTube() {
    setTimeout(function() {
      window.location.href = 'https://www.youtube.com/watch?v=izGwDsrQ1eQ&ab_channel=georgemichaelVEVO';
    }, 5000);
  }
</script>

